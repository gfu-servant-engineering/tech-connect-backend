{
  "connection": "default",
  "collectionName": "blogpages",
  "info": {
    "name": "Blogpage"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "templateKey": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "date": {
      "type": "date"
    },
    "Description": {
      "type": "richtext"
    },
    "short_description": {
      "type": "text"
    }
  }
}
